{
  "name": "@gurupras/librsync-wasm",
  "version": "0.1.0",
  "description": "librsync implemented as a WASM module with wrapper classes",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && vite build",
    "test": "vitest run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "express": "^4.18.2",
    "portfinder": "^1.0.32",
    "puppeteer": "^20.7.3",
    "typescript": "^5.1.5",
    "vite-plugin-dts": "^3.0.0-beta.3",
    "vitest": "^0.32.2"
  },
  "files": [
    "*.ts",
    "!*.test.ts",
    "dist/",
    "librsync.wasm",
    "wasm_exec.js",
    "*.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./librsync.wasm": "./librsync.wasm",
    "./wasm_exec.js": "./wasm_exec.js"
  }
}
